{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**", "out/**"],
			"env": [
				"CONVEX_URL",
				"NEXT_PUBLIC_CONVEX_URL",
				"NEXT_PUBLIC_APP_URL",
				"GITHUB_APP_ID",
				"GITHUB_CLIENT_ID"
			]
		},
		"lint": {
			"outputs": []
		},
		"test": {
			"dependsOn": ["build"],
			"outputs": []
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}




