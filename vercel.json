{
	"functions": {
		"apps/api/api/index.ts": {
			"runtime": "nodejs18.x",
			"memory": 1024,
			"maxDuration": 30
		}
	},
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/apps/api/api/index.ts"
		}
	]
}
{
	"version": 2,
	"builds": [
		{
			"src": "apps/web/next.config.ts",
			"use": "@vercel/next"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/apps/web/$1"
		}
	]
}
